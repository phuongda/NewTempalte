- const base = "."
- const home = true
- const canonical = "/"
- const page = "news"

doctype html
html(lang="ja")
    include ./partials/_head
    body
        include ./layouts/_header
        include ./components/_bl-banner
        +blockBanner("NEWS", "お知らせ", "assets/img/news/banner.jpg", [ { "link": "#", "title": "株式会社弥生金属" }, { "link": "#", "title": "お知らせ" }, { "link": "", "title": "ホームページをリニューアルしました" } ])
        img.lazy(data-src="assets/img/news/img-2.jpg")
    include ./layouts/_footer